            "pages": [
              "android-sdk/index",
              "android-sdk/installation",
              "android-sdk/dynamic-strings",
              "android-sdk/translations",
              "android-sdk/usage",
              "android-sdk/best-practices"
            ]
          },
          {
            "group": "iOS SDK",
            "pages": [
              "ios-sdk/index",
              "ios-sdk/installation",
              "ios-sdk/dynamic-strings",
              "ios-sdk/translations",
              "ios-sdk/usage",
              "ios-sdk/best-practices"
            ]
          },
          {
            "group": "Web SDK",
            "pages": [
              "web-sdk/index",
              "web-sdk/installation",
              "web-sdk/dynamic-strings",
              "web-sdk/translations",
              "web-sdk/react-hooks",
              "web-sdk/nextjs",
              "web-sdk/usage",
              "web-sdk/best-practices"
            ]
          },
          {
            "group": "Dashboard",
            "pages": [
              "dashboard-guides/dashboard-overview",
              "dashboard-guides/authentication",
              "dashboard-guides/applications",
              "dashboard-guides/strings",
              "dashboard-guides/languages",
              "dashboard-guides/experiments",
              "dashboard-guides/faqs",
              "dashboard-guides/api-tokens",
              "dashboard-guides/settings"
