{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Stringboot",
  "colors": {
    "primary": "#FE5D05",
    "light": "#FE5D05",
    "dark": "#FE5D05"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "book",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "core-concepts",
              "faq"
            ]
          },
          {
            "group": "Android SDK",
            "pages": [
              "android-sdk/index",
              "android-sdk/installation",
              "android-sdk/usage",
              "android-sdk/ab-testing",
              "android-sdk/faq-provider",
              "android-sdk/troubleshooting"
            ]
          },
          {
            "group": "iOS SDK",
            "pages": [
              "ios-sdk/index",
              "ios-sdk/installation",
              "ios-sdk/usage",
              "ios-sdk/ab-testing",
              "ios-sdk/faq-provider",
              "ios-sdk/troubleshooting"
            ]
          },
          {
            "group": "Web SDK",
            "pages": [
              "web-sdk/index",
              "web-sdk/quickstart",
              "web-sdk/vanilla-js",
              "web-sdk/react",
              "web-sdk/vue",
              "web-sdk/nextjs",
              "web-sdk/faq-provider",
              "web-sdk/advanced",
              "web-sdk/troubleshooting"
            ]
          },
          {
            "group": "Dashboard",
            "pages": [
              "dashboard-guides/dashboard-overview",
              "dashboard-guides/authentication",
              "dashboard-guides/applications",
              "dashboard-guides/strings",
              "dashboard-guides/languages",
              "dashboard-guides/experiments",
              "dashboard-guides/faqs",
              "dashboard-guides/api-tokens",
              "dashboard-guides/settings"
            ]
          }
        ]
      },
      {
        "tab": "Cookbook",
        "icon": "book-open",
        "groups": [
          {
            "group": "Overview",
            "pages": ["cookbook/index"]
          },
          {
            "group": "Recipes",
            "pages": [
              "cookbook/web-samples",
              "cookbook/android-samples",
              "cookbook/ios-samples"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Support",
          "href": "mailto:developer@stringboot.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Get API Key",
        "href": "https://dashboard.stringboot.com/tokens"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Signup",
      "href": "https://dashboard.stringboot.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Stringboot-SDK",
      "linkedin": "https://linkedin.com/company/stringboot"
    }
  }
}
