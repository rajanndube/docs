     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Stringboot-SDK",
      "linkedin": "https://linkedin.com/company/stringboot"
    }
  }
