{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Stringboot",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "core-concepts"
            ]
          },
          {
            "group": "Web SDK",
            "pages": [
              "web-sdk/installation",
              "web-sdk/usage",
              "web-sdk/react-hooks",
              "web-sdk/best-practices"
            ]
          },
          {
            "group": "Android SDK",
            "pages": [
              "android-sdk/installation",
              "android-sdk/usage",
              "android-sdk/best-practices"
            ]
          },
          {
            "group": "iOS SDK",
            "pages": [
              "ios-sdk/installation",
              "ios-sdk/usage",
              "ios-sdk/best-practices"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "advanced/error-handling",
              "advanced/caching",
              "advanced/troubleshooting"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/stringboot",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@stringboot.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://dashboard.stringboot.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://dashboard.stringboot.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/stringboot",
      "linkedin": "https://linkedin.com/company/stringboot"
    }
  }
}
